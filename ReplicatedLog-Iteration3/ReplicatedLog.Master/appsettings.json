{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Secondaries": {
        "Urls": [ "https://localhost:7084", "https://localhost:7085" ]
    },
    "HeartBeatIntervalInSeconds": 10,
    "RetryPolicy": {
        "MaxRetries": 5,
        "BaseDelayMs": 1000
    }
}
